// prettier-ignore
{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "*":["src/*"],
      "@components/*":["src/components/*"],
      "@views/*":["src/views/*"],
      "@assets/*":["src/assets/*"],
      "@utils/*":["src/utils/*"],
      "@context/*":["src/context/*"],
      "@hooks/*":["src/hooks/*"],
    }
  }
}
